{
    "source": "./src",
    "destination": "./out/esdoc",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "test": {
                    "type": "mocha",
                    "source": "./test",
                    "includes":["\\.js"]
                }
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "objectRestSpread": true
            }
        }
    ]
}
